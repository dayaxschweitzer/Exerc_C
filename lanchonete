#include <stdio.h>
#include <stdlib.h>

int main()
{
    int cod, quant;
    double pagar;

    printf("Codigo do produto comprado: ");
    scanf("%d", &cod);

    printf("Quantidade comprada: ");
    scanf("%d", &quant);

    if (cod == 1) {
        pagar = quant * 5.00;
    }
    else if (cod == 2) {
        pagar = quant * 3.50;
    }
    else if (cod == 3) {
        pagar = quant * 4.80;
    }
    else if (cod == 4) {
        pagar = quant * 8.90;
    }
    else if (cod == 5) {
        pagar = quant * 7.32;
    }

	printf("Valor a pagar: R$ %.2lf\n", pagar);

	return 0;
}

