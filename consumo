#include <stdio.h>
#include <stdlib.h>

int main()
{

double distancia, qtde_combustivel, consumo;

printf("Distancia percorrida: ");
scanf("%lf", &distancia);

printf("Combust√≠vel gasto: ");
scanf("%lf", &qtde_combustivel);

consumo = distancia / qtde_combustivel;

printf("Consumo medio = %.3lf\n", consumo);


    return 0;
}

